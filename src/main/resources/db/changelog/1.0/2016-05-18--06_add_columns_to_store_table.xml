<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet id="add columns: address, opendate, closedate, note " author="flexjoy" context="scheme">
		<addColumn tableName="store">
			<column name="address" type="varchar(150)"/>
			<column name="opendate" type="datetime"/>
			<column name="closedate" type="datetime"/>
			<column name="note" type="varchar(255)"/>
		</addColumn>
	</changeSet>

</databaseChangeLog>
